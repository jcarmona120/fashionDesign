{
  "connection": "default",
  "collectionName": "feedposts",
  "info": {
    "name": "feedpost",
    "description": "This is for the feed."
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "post_title": {
      "default": "",
      "type": "text"
    },
    "thumbnail": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "presented_by": {
      "default": "",
      "type": "string"
    },
    "date_published": {
      "default": "",
      "type": "date"
    },
    "published": {
      "default": "",
      "type": "text"
    }
  }
}